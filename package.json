{
  "name": "phpconf-tw-2015",
  "version": "1.0.0",
  "description": "website for PHPConf Taiwan 2015",
  "scripts": {
    "start": "babel-node src/server.js",
    "build": "gulp build",
    "lint": "eslint src",
    "deploy": "gh-pages -d build",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PHPConf-TW/website-2015.git"
  },
  "keywords": [
    "phpconf"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PHPConf-TW/website-2015/issues"
  },
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.0.5",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.1.2",
    "coveralls": "^2.11.3",
    "css-loader": "^0.15.2",
    "del": "^1.2.0",
    "eslint-config-airbnb": "0.0.7",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.1",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.2.1",
    "gulp-imagemin": "^2.3.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-rev": "^5.1.0",
    "gulp-rev-replace": "^0.4.2",
    "gulp-size": "^1.2.3",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.5.2",
    "json-loader": "^0.5.2",
    "less-loader": "^2.2.0",
    "minimist": "^1.1.1",
    "react-hot-loader": "^1.2.7",
    "run-sequence": "^1.1.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2",
    "yaml-loader": "^0.1.0"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "google-map-react": "^0.4.1",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.13.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-script-preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
